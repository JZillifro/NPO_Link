{
   "AWSEBDockerrunVersion": 2,
   "containerDefinitions": [{
      "essential": true,
      "memory": 256,
      "image": "gerardomares/npolink:frontend",
      "mountPoints": [{
         "containerPath": "/usr/frontend",
         "sourceVolume": "_frontend"
      }],
      "name": "frontend",
      "portMappings": [{
         "containerPort": 3000,
         "hostPort": 80
      }]
   }],
   "family": "",
   "volumes": [{
      "host": {
         "sourcePath": "./"
      },
      "name": "_frontend"
   }]
}
